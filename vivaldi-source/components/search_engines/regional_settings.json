{
  // NOTE(konrad@vivaldi.com): This file is not used by us, but must exist
  // for compatibility. When running with ‘--disable-vivaldi’ (e.g. tests),
  // we need to make sure there is at least one language for the locale.
  // For simplicity, we can remove all elements from kRegionalSettings so that
  // the code always uses the default regional settings.
  "elements": {
    // Fallback / default country. Occupies a user-assignable alpha-2 code,
    // representing unknown or invalid territory.
    // See https://en.wikipedia.org/wiki/ISO_3166-1_alpha-2#ZZ
    "ZZ": {  // Default
      "search_engines": [
        "&google"
      ]
    }
  },
  "aggregation": {
    "type": "map",
    "name": "kRegionalSettings",
    "export_items": false,
    "map_key_type": "country_codes::CountryId",
    "map_aliases": {
    }
  }
}
